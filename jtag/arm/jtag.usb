TYPE=bulk
PACKETSIZE=64
ENDENDPOINT

ENDPOINT=2
DIR=in
TYPE=bulk
PACKETSIZE=64
ENDENDPOINT


ENDINTERFACE
ENDCONFIG
ENDDEVICE

