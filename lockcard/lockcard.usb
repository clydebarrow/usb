;	USB descriptors for HI-TECH ZipCard

DEVICE=lockcard
USB=2.0
CLASS=0,0,0
PACKETSIZE=64
VID=0x1725
PID=0x0002
RELEASE=0x200
MANUFACTURER=Generic
PRODUCT=HI-TECH LockCard
SERIAL=423456789ABCDEF

; Configuration info

CONFIG=Default
POWER=self,1
WAKEUP=no

; Interface info

INTERFACE=Mass Storage
CLASS=0x8,0x6,0x50

;Endpoints

ENDPOINT=1
DIR=out
TYPE=bulk
PACKETSIZE=64
ENDENDPOINT

ENDPOINT=2
DIR=in
TYPE=bulk
PACKETSIZE=64
ENDENDPOINT

ENDINTERFACE
ENDCONFIG
ENDDEVICE
