;	USB descriptors for HI-TECH JTAG adapter

DEVICE=jtag
USB=1.1
CLASS=0,0,0
PACKETSIZE=64
VID=0x1725
PID=0x0002
RELEASE=0x101
MANUFACTURER=HI-TECH Software
PRODUCT=USB JTAG Adapter
;SERIAL=

; Configuration info

CONFIG=Default
POWER=bus,20
